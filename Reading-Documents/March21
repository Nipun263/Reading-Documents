	<test name="Progression Test Cases" verbose="9">
		

		<classes>
			<class name="LoginSuite.Login">
			<methods>
			<include name="TestMethods which you want to run"></include>
			</methods>
			</class>
			
				
		</classes>


	</test> <!-- Default test -->
</suite> <!-- Default suite -->

